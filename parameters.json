{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "westus"
        },
        "publicIPAddressName": {
            "value": "PUBLICIP-NAME"
        },
        "publicIPNewOrExisting": {
            "value": "new"
        },
        "publicIPAllocationMethod": {
            "value": "Dynamic"
        },
        "vmName": {
            "value": "VM-NAME"
        },
        "adminUsername": {
            "value": "ADMIN-USERNAME"
        },
        "authenticationType": {
            "value": "password"
        },
        "adminPassword": {
            "value": "ADMIN-PASSWORD"
        },
        "adminSshKey": {
            "value": "ADMIN-SSHKEY"
        },
        "vmSize": {
            "value": "Standard_DS3_v2"
        },
        "imageVersion": {
            "value": "10.1.0"
        },
        "srcIPInboundNSG": {
            "value": "0.0.0.0/0"
        },
        "nsgName": {
            "value": "NSG-NAME"
        },
        "virtualNetworkName": {
            "value": "VNET-NAME"
        },
        "virtualNetworkAddressPrefix": {
            "value": "192.168.0.0/16"
        },
        "vnetNewOrExisting": {
            "value": "new"
        },
        "virtualNetworkExistingRGName": {
            "value": ""
        },
        "subnet0Name": {
            "value": "Mgmt"
        },
        "subnet1Name": {
            "value": "Untrust"
        },
        "subnet2Name": {
            "value": "Trust"
        },
        "subnet0Prefix": {
            "value": "192.168.0.0/24"
        },
        "subnet1Prefix": {
            "value": "192.168.1.0/24"
        },
        "subnet2Prefix": {
            "value": "192.168.2.0/24"
        },
        "bootstrap": {
            "value": "no"
        },
        "customData": {
            "value": "storage-account=None,access-key=None,file-share=None,share-directory=None"
        }
    }
}